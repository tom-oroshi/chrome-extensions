{
  "name": "chrome-dashboard-looper",
  "short_name": "dashboard looper",
  "theme_color": "#0ea5e9",
  "permissions": [
    "storage"
  ],
  "icons": [
    {
      "src": "/images/icons/icon-512x512.png",
      "type": "image/png",
      "sizes": "512x512"
    }
  ],
  "background_color": "#5eead4",
  "display": "fullscreen",
  "scope": "/",
  "start_url": "/index.html",
  "storage": {
    "managed_schema": "/schema.json"
  }
}